paths:
{{#each paths as |path i|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]
tags: {{tags}}
publisher_pipeline.disable_host: {{nList tags forwarded"}}
processors:
  - decode_json_fields:
      fields: [message]
      target: zoom
  - add_locale: ~
  - add_fields:
      target: ''
      fields:
        ecs.version: 1.5.0